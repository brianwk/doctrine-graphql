{
  "name": "ratehub/doctrine-graphql",
  "description": "A GraphQL Doctrine ORM bridge",
  "license": "MIT",
  "keywords": [
    "doctrine",
    "graphql",
    "orm",
    "data mapper",
    "database"
  ],
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Nathanael Morrison",
      "email": "nathanael@ratehub.ca"
    }
  ],
  "repositories":[
    {
      "type": "git",
      "url": "https://github.com/ratehub/doctrine2.git"
    },
    {
      "type": "git",
      "url": "https://github.com/ratehub/laravel-doctrine.git"
    },
    {
      "type": "git",
      "url": "https://github.com/ratehub/Codeception.git"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "doctrine/orm": "dev-master",
    "webonyx/graphql-php": "^0.9",
    "laravel/framework": "5.3.*",
    "laravel-doctrine/orm": "dev-ratehub-laravel-doctrine"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "codeception/codeception": "dev-laravel5-fix"
  },
  "autoload": {
    "psr-4": {
      "RateHub\\GraphQL\\": "src/"
    }
  }
}